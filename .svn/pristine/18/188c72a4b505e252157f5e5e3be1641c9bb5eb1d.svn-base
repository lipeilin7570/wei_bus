* {
	padding: 0;
	margin: 0;
	font-family: "Microsoft YaHei";
	font-size: 16px;
}

html {
	background: url('../imgs/inco/bg_big.jpg') no-repeat;
	background-size: cover;
	height: 100%;
}

#page {
	width:412px;
	height:479px;
	border:1px solid #ddd;
	margin-left:60%;
	margin-top:5%;
	background-color:#fff;
}

#login {
	text-align:center;
	margin-top:10%;
}

#login ul li {
	list-style-type: none;
	margin-bottom: 15px;
}

#userName, #password {
	margin-top: 30px;
	height: 40px;
	width: 319px;
	padding-left:30px;
	border-radius:5px;
}

.button {
	width: 188px;
	height: 41px;
	background-color: #6EB92B;
	margin-top: 60px;
}



